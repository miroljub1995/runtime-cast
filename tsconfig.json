{
  "compilerOptions": {
    "outDir": "./lib/",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ESNext",
    "declaration": true,
    "skipLibCheck": true,
    "strict": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true
  },
  "include": [
    "src",
    "test"
  ]
}